	keywords = {Computational Biology, Databases, Genetic, Genetic Association Studies, Genetic Diseases, Inborn, Genetic Predisposition to Disease, Genetics, Medical, Genomics, Humans, Information Storage and Retrieval, Inheritance Patterns, Internet},
	pages = {D1038--D1043},
}

