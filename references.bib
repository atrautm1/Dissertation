	urldate = {2019-01-15},
	journal = {Journal of Clinical Gastroenterology},
	author = {Azpiroz, Fernando and Molne, Laura and Mendez, Sara and Nieto, Adoración and Manichanh, Chaysavanh and Mego, Marianela and Accarino, Anna and Santos, Javier and Sailer, Manuela and Theis, Stephan and Guarner, Francisco},
	month = aug,
